    "#### 5. Write the header and signal"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import wave\n",
    "import struct\n",
    "\n",
    "filename = \"signal.wav\"\n",
    "\n",
    "amplitude = 25000             # range: -32768 ~ 32768 (16-bits)\n",
    "frequency = 200               # (Hz)\n",
    "duration = 10                 # (s)\n",
    "fs = 44100                    # (Hz) (for a common audio file)\n",
    "num_samples = duration * fs\n",
    "\n",
    "num_channels = 1\n",
    "sampwidth = 2\n",
    "num_frames = num_samples\n",
    "comptype = \"NONE\"\n",
    "compname = \"not compressed\"\n",
    "\n",
    "t = np.linspace(0, duration, num_samples, endpoint = False)\n",
    "x = amplitude * np.cos(2 * np.pi * frequency * t)\n",
    "\n",
    "wav_file = wave.open(filename, 'w')\n",
    "\n",
    "# Write the header\n",
    "wav_file.setparams((num_channels, sampwidth, fs, num_frames, comptype, compname))\n",
    "\n",
    "# Write the signal\n",
    "for s in x:\n",
    "    # pack('h', int(s)): it transforms int(s) to 16-bits (2-bytes) format\n",
    "    wav_file.writeframes(struct.pack('h', int(s)))\n",
    "    \n",
    "wav_file.close()"
   ]
  },
